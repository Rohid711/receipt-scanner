# Telegram Bot Token - Get this from BotFather (https://t.me/botfather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Server configuration
PORT=3000

# Your Telegram Mini App URL
# For development, you need to use HTTPS URLs
# Options:
# 1. Deploy to a service like Vercel, Netlify, etc. and use that URL
# 2. Use a service like ngrok to expose your local server: https://yourngrok.ngrok.io
# 3. Use a placeholder for testing the bot interaction: https://t.me/your_bot_username
WEB_APP_URL=https://t.me/your_bot_username

# Google Gemini API Key for receipt scanning
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here 